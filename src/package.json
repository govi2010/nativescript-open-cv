{
  "name": "nativescript-open-cv",
  "version": "1.0.0",
  "description": "Your awesome NativeScript plugin.",
  "main": "open-cv",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "5.4.0",
      "ios": "5.4.0"
    },
    "hooks": [
      {
        "type": "after-prepare",
        "script": "after-prepare.js",
        "inject": true
      }
    ]
  },
  "directories": {
    "scripts": "scripts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/govi2010/nativescript-open-cv.git"
  },
  "scripts": {
    "postinstall": "node postinstall.js",
    "preuninstall": "node preuninstall.js",
    "tsc": "npm i && tsc",
    "build": "npm run build-src &&  npm run tsc && npm run build.native",
    "build-src": "bash  src-native/android/build.sh && bash src-native/ios/build.sh",
    "build.native": "node scripts/build-native.js",
    "postclone": "npm i && node scripts/postclone.js",
    "tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\"",
    "plugin.tscwatch": "npm run tsc -- -w",
    "ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**' --exclude '**/platforms/**'",
    "prepack": "npm run build.native",
    "demo.ios": "npm run tsc && cd ../demo && tns debug ios  --debug-brk",
    "demo.android": "npm run tsc && cd ../demo && tns debug android --emulator",
    "demo.reset": "cd ../demo && npx rimraf -- hooks node_modules platforms package-lock.json",
    "plugin.prepare": "npm run build && cd ../demo && tns plugin remove nativescript-open-cv && tns plugin add ../src",
    "clean": "npm run demo.reset && npx rimraf -- node_modules package-lock.json && npm i"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS"
  ],
  "author": {
    "name": "Your Name",
    "email": "youremail@yourdomain.com"
  },
  "bugs": {
    "url": "https://github.com/govi2010/nativescript-open-cv/issues"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/govi2010/nativescript-open-cv",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-core-modules": "^6.0.1",
    "tns-platform-declarations": "^6.0.1",
    "typescript": "~3.3.3",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.1",
    "semver": "^5.6.0"
  },
  "dependencies": {
    "nativescript-hook": "0.2.5"
  },
  "bootstrapper": "nativescript-plugin-seed"
}
